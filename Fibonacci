
public class Fibonacci {
  
	public static void main (String [] args){
		
		int a = 1;
		int b = 2;
		int c;
		int sum = 2;
		
		while (a + b < 4000000){
			
			c = (a + b);
			
			if(c % 2 == 0){
				
				sum += c;
				
			}
			
			a = b;
			b = c;
			
			}
			
			System.out.println(sum);
			
		}
		
	}
