
public class SumSquareDifference {
  
	public static void main(String[] args){
		
		int sum1 = 0;
		int sum2 = 0;
		int x = 0;
		
		for(int i = 1; i<101; i++){
			
			sum1 += (Math.pow(i,2));
				
		}
			System.out.println(sum1);
			
			
		for (int i = 1; i<101; i++){
			
			x += i;
			System.out.println(x);
			
			
			
		}
			sum2 += Math.pow(x,2);
			System.out.println(sum2);
			
			System.out.println(sum2 - sum1);
		
	}
	
}
